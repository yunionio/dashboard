{
  "name": "onecloud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node --max_old_space_size=${MAX_OLD_SPACE_SIZE:-4096} node_modules/@vue/cli-service/bin/vue-cli-service.js build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "setup:dev": "sh ./scripts/setup.dev.sh",
    "checkout": "chmod +x ./scripts/checkout.sh && sh ./scripts/checkout.sh $@",
    "sync": "chmod +x ./scripts/sync-modules.sh && sh ./scripts/sync-modules.sh $@"
  },
  "dependencies": {
    "animejs": "^3.1.0",
    "ant-design-vue": "^1.5.0",
    "axios": "^0.19.0",
    "clipboard": "^2.0.4",
    "codemirror": "^5.52.2",
    "core-js": "^3.4.4",
    "echarts": "^4.4.0",
    "js-base64": "^2.5.1",
    "js-cookie": "^2.2.1",
    "jsrsasign": "^8.0.12",
    "lodash": "^4.17.15",
    "marked": "^0.8.0",
    "moment": "^2.24.0",
    "numerify": "^1.2.9",
    "qs": "^6.8.0",
    "ramda": "^0.26.1",
    "resize-detector": "^0.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "socket.io-client": "^2.3.0",
    "v-charts": "^1.19.0",
    "vue": "^2.6.10",
    "vue-cropperjs": "^4.0.1",
    "vue-grid-layout": "^2.3.7",
    "vue-i18n": "^8.14.0",
    "vue-router": "^3.1.3",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.2",
    "vxe-table": "^2.8.33",
    "vxe-table-plugin-antd": "^1.5.8",
    "xe-utils": "^2.3.0",
    "xterm": "3.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "svg-sprite-loader": "^4.1.6",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
